<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BookingApplication.Views.ProprietatePage">
    <ContentPage.Content>
        <StackLayout Margin="20">
            <Label  Text="{Binding Nume}" HeightRequest="100" />
            
            <Label Text="{Binding Judet}"/>
            
            <Label Text="{Binding Oras}"/>
            
            <Label Text="{Binding Strada}" />
           
            <Label Text="{Binding Numar}"/>
            
            <Label Text="{Binding Pret}"/>
            <Button Text="Adauga rezervare"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>